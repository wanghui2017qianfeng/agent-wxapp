<van-grid column-num="4" border="{{ false }}">
  <van-grid-item use-slot wx:for="{{ list }}" wx:key="index" class="grid" bindtap="goBuild" data-index="{{index}}">
    <image src="{{item.src}}" class="iconBox"></image>
    <view style="font-size:29rpx;">{{item.text}}</view>
  </van-grid-item>
</van-grid>

<view class="bobao flex">
  <view class="border-r">
    <image src="../../assets/img/bobao.png"></image>
  </view>
  <view class="person">
    <image src="../../assets/img/tuxiang.png"></image>
    <view>李晓琪</view>
  </view>
  <view class="info">恭喜新城中心花园店的李小四签约新城中心花园 55.00万元房源
  </view>
</view>

<view class="msg-center">
<view class="title"><image src="../../assets/img/laba.png"></image>消息中心</view>

