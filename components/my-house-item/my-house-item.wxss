/* components/my-house-item/my-house-item.wxss */
@import "../../app.wxss";
.house-item {
margin:0 50rpx;
padding:54rpx 0;
border-bottom: 1rpx solid #eeeeee;

}
.right-box{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.house-item .roomImg{
  width:218rpx;
  height:181rpx;
  margin-right:18rpx;
}

.house-item .title{
  font-size: 30rpx;
  color:#333;
  font-weight:bold;
}
.house-item .info{
  color:#9A9A9A;
  font-size: 24rpx;
}
.house-item .tag{
  background: #F2F3F5;
  border-radius:5rpx;
  color:#889AAE;
  font-size: 18rpx;
  padding:10rpx 6rpx 6rpx;
  margin-right:16rpx;
}
.house-item .price{
  font-size: 25rpx;
  color:#333333;
}

.house-item .price{
  color:#EE3C52;
  font-size: 30rpx;
  margin:10rpx 38rpx 0 24rpx

}

.unit{
  font-size: 20rpx;
  margin-top:18rpx;
}

.checkImg image{
  width:28rpx;
  height:28rpx;
  margin-top:70rpx;
  margin-right:22rpx
}

